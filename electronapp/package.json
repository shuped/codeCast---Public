{
  "name": "node-electron",
  "version": "1.0.0",
  "description": "codeCast Broadcaster electron app, with xterm and react.",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run rebuild-pty",
    "build-start": "npm run build && npm start",
    "start": "electron . --ignore-gpu-blacklist",
    "build": "webpack",
    "rebuild-pty": "electron-rebuild -f -w node-pty",
    "sass": "sass --watch src/styles/scss/index.scss:public/styles/css/index.css",
    "build-prod": "cp -r . ../electron-v1.8.4/codeCast.app/Contents/Resources/app/"
  },
  "proxy": "http://codeCastServer-prod.us-west-2.elasticbeanstalk.com/",
  "repository": "https://github.com/benji-leboe/codeCast",
  "author": "Spencer Shupe, Benji Leboe, Erik Mackie",
  "dependencies": {
    "antd": "^3.10.3",
    "axios": "~0.18.0",
    "browserslist": "^4.3.4",
    "electron": "^2.0.9",
    "less": "~3.8.1",
    "node-pty": "^0.7.8",
    "postcss-loader": "~3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "sass": "^1.14.3",
    "socket.io-client": "^2.1.1",
    "term-size": "^1.2.0",
    "uuid": "~3.3.2",
    "xterm": "3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "~1.11.0",
    "css-loader": "~1.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "~1.8.2",
    "file-loader": "~2.0.0",
    "html-webpack-plugin": "~3.2.0",
    "less-loader": "~4.1.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "~4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "~0.12.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
